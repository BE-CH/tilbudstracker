{
  "name": "tilbudstracker",
  "version": "1.0.0",
  "description": "A tracker that tracks all tilbuds",
  "private": true,
  "workspaces": [
    "api",
    "client"
  ],
  "scripts": {
    "startServer": "node ./api/bin/www",
    "startServerDev": "nodemon ./api/bin/www",
    "startClientDev": "cd ./client && next dev -p 4000",
    "buildClient": "cd ./client && next build -p 4000",
    "startClient": "cd ./client && next start -p 4000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BE-CH/tilbudstracker.git"
  },
